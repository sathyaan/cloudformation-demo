{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "S3Bucket": {
            "Type": "AWS::S3::Bucket"
        },
        "AlertSNSTopic" : {
        "Type" : "AWS::SNS::Topic",
        "Properties" : {
          "Subscription" :  [{
            "Endpoint" : "sathya@gmail.com,
            "Protocol" : "email"
          }]
        }
      }
    },
    "Outputs": {
        "BucketName": {
            "Value": {
                "Ref": "S3Bucket"
            },
            "Description": "Name of sample Amazon S3 Bucket"
        }
    }
}
